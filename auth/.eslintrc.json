{
    "extends": ["airbnb", "prettier"],

    "env": {
        "es6": true,
        "browser": true,
        "mocha": true
    },
    "rules": {
        "no-unused-vars": "warn",
        "func-names": "off",
        "no-process-exit": "off",
        "object-shorthand": "off",
        "class-methods-use-this": "off"
      }
}